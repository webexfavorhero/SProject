{{#if model.aboutUs}}
{{app-header-subheader}}
{{partial "partials/banner-about"}}

<!-- SUBHEADER BEGIN -->
<div class="outer-subheader">
    <div class="site-subheader bg-bluelight clearfix">
        <div class="container">
            
            <div class="subheader-navigation">
                <ul>
                    <li><a href="about#about-introduce">we are sciensa</a></li>
                    <li><a href="about#about-mission">mission</a></li>
                    <li><a href="about#about-culture">culture</a></li>
                    <li><a href="about#about-office">our office</a></li>
                </ul>
            </div><!-- / .subheader-navigation -->

            <div class="rwd-internal-header clearfix">
                <span class="btn-showing-internal"><i class="fa fa-bars"></i> pages internal</span>
                
                <div class="clearfix"></div>
                <ul class="rwd-internal-navigation"></ul>
            </div>

            <div class="pull-right">
                <a href="#" class="btn btn-white text-bluelight">contact us</a>
            </div>

        </div>
    </div>            
</div>
<!-- SUBHEADER END -->

<!-- SECTION ABOUT-INTRODUCE BEGIN -->
<section id="about-introduce" class="content-section content-aboutintroduce clearfix">
  <div class="container">

    <div class="row">
      <div class="col-md-8 col-md-offset-2">
        {{#each model.weAreSciensa as |sciensa|}}
        <header class="content-header">
          <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{sciensa.gallery}}" alt="" class="icon-content-header" width="186">
          <h2 class="title-content">{{sciensa.title}}</h2>
        </header>

        <p class="text-content-section">{{sciensa.description}}</p>
        {{/each}}
      </div>
    </div><!-- / .row -->

  </div>
</section>
<!-- SECTION ABOUT-INTRODUCE END -->

<!-- SECTION ABOUT-MISSION BEGIN -->
<section id="about-mission" class="content-section bg-gradient-purpleblue content-aboutmission clearfix">
  <div class="container">

    <div class="row">
      <div class="col-md-10 col-md-offset-1">
        {{#each model.missionTitle as |missTitle|}}
        <header class="content-header">
          <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{missTitle.title}}</h2>
        </header>

        <p class="text-content-section wow fadeInUp" style="max-width: 855px; font-size: 24px;" data-wow-duration="1s" data-wow-delay="200ms">{{missTitle.description}}</p>
        {{/each}}
      </div>
    </div><!-- / .row -->

    <div class="clearfix"></div>
    <div class="space100"></div>

    <div class="row aboutmission-container">
      {{#each model.mission as |miss|}}
      <div class="col-md-3 item-mission clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">
        <div class="subitem-mission">
          <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{miss.gallery}}" alt="" width="125">
          <h3><a href="#">{{miss.itemTitle}}</a></h3>
        </div>
      </div><!-- / .item-mission -->
      {{/each}}
    </div><!-- / .aboutmission-container -->

  </div>
</section>
<!-- SECTION MISSION END -->

<!-- SECTION ABOUT-CULTURE BEGIN -->
<section id="about-culture" class="content-section content-aboutculture clearfix">
  <div class="container">

    <div class="row">
      <div class="col-md-10 col-md-offset-1">
        {{#each model.ourCulture as |cult|}}
        <header class="content-header">
          <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{cult.title}}</h2>
        </header>

        <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms" style="min-width: 650px;">{{cult.description}}</p>
        {{/each}}
      </div>
    </div>

    <div class="clearfix"></div>
    <div class="space100"></div>

    <div class="row aboutculture-container">
      {{#each model.ourCulture as |cult|}}
      <div class="col-md-12 item-culture clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">
        <div class="">
          <figure>
            <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{cult.gallery}}" alt="">
          </figure>
        </div>
      </div><!-- / .item-culture -->
      {{/each}}

    </div><!-- / .culture-container -->

  </div>
</section>
<!-- SECTION CULTURE END -->

<!-- SECTION ABOUT-OFFICE BEGIN -->
<section id="about-office" class="content-section content-aboutoffice nopaddbottom clearfix">

  <div class="bg-bluelight">
    <div class="container">

      <div class="space100"></div>
      <div class="clearfix"></div>

      <div class="row">
        <div class="col-md-6 col-md-offset-3">
          {{#each model.ourOfficeTitle as |offTitle|}}
          <header class="content-header">
            <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{offTitle.title}}</h2>
          </header>

          <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{offTitle.description}}</p>
          {{/each}}
        </div>
      </div><!-- / .row -->

      <div class="clearfix"></div>
      <div class="space100"></div>
      <div class="space100"></div>
      <div class="space100"></div>


    </div><!-- / .container -->
  </div>

  <div class="content-float bg-white">
    <div class="container">

      <div class="row">
        {{#if model.ourOffice}}
          {{about-office off=model.ourOffice}}
        {{/if}}
      </div>

      <div class="clearfix"></div>
      <div class="space100"></div>
      <div class="space100"></div>
    </div>
  </div><!-- / .content-float -->

</section>
<!-- SECTION ABOUT-OFFICE END -->

<!-- SECTION INFORMATION BEGIN -->
<section id="information" class="content-section content-information bg-bluedark clearfix">
  <div class="container">

    <div class="row">
      <div class="col-md-8 col-md-offset-2">
        {{#each model.knowMore as |more|}}
        <header class="content-header">
          <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{more.title}}</h2>
        </header>

        <div class="clearfix"></div>
        <div class="text-center">
          <a href="#" class="btn btn-bluelight wow fadeInUp" data-wow-duration="1s" data-wow-delay="300ms">contact us</a>
          <div class="space45"></div>
          <p class="text-uppercase">{{more.phone}}</p>
        </div>
        {{/each}}
      </div>
    </div>

  </div>
</section>
<!-- SECTION INFORMATION END -->

{{app-footer}}

{{else}}
    {{partial "partials/lazy-loading"}}
{{/if}}