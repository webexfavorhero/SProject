{{app-header}}
<!-- BANNER HOME BG BEGIN -->
{{#each model.ourClientsBanner as |clientsBanner|}}
<section class="content-section-bg clearfix" style="background-image: url('http://ec2-54-83-80-20.compute-1.amazonaws.com{{clientsBanner.gallery}}');">
    <div class="mask-banner"></div>
    <div class="container">
        <div class="caption-banner" style="max-width: 680px;">
            <h1 class="title-banner wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{clientsBanner.title}}</h1>
            <p class="text-banner wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms" >{{clientsBanner.description}}</p>
        </div><!-- / .caption-banner -->
    </div>

    <a class="link-arrow">
        <i class="fa fa-angle-double-down"></i>
    </a>
</section>
{{/each}}
<!-- BANNER HOME BG END -->

<!-- SUBHEADER BEGIN -->
<div class="outer-subheader">
    <div class="site-subheader bg-blue clearfix">
        <div class="container">
            
            <div class="subheader-navigation">
                <ul>
                    <li><a href="#">we are sciensa</a></li>
                    <li><a href="#">mission</a></li>
                    <li><a href="#">culture</a></li>
                    <li><a href="#">what we do</a></li>
                    <li><a href="#">latested cases</a></li>
                </ul>
            </div><!-- / .subheader-navigation -->

            <div class="rwd-internal-header clearfix">
                <span class="btn-showing-internal"><i class="fa fa-bars"></i> pages internal</span>
                
                <div class="clearfix"></div>
                <ul class="rwd-internal-navigation"></ul>
            </div>

            <div class="pull-right">
                <a href="#" class="btn btn-white text-blue">contact us</a>
            </div>

        </div>
    </div>
</div>
<!-- SUBHEADER END -->

<!-- SECTION CLIENT-INTRODUCE BEGIN -->
<section id="client-introduce" class="content-section content-clientintroduce clearfix">
    <div class="container">

        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                {{#each model.aboutClients as |aClients|}}
                <header class="content-header">
                    <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{aClients.gallery}}" alt="" class="icon-content-header wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms" width="186">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{aClients.title}}</h2>
                </header>

                <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{aClients.description}}</p>
                {{/each}}
            </div>
        </div><!-- / .row -->

    </div>
</section>
<!-- SECTION CLIENT-INTRODUCE END -->

<!-- SECTION CLIENT-OUR BEGIN -->
<section id="client-our" class="content-section content-clientour bg-gray clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                {{#each model.ourClientsTitle as |clientsTitle|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{clientsTitle.title}}</h2>
                </header>
                
                <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{clientsTitle.description}}</p>
                {{/each}}
            </div>
        </div>

        <div class="clearfix"></div>
        <div class="space100"></div>

        <div class="row clientour-container">
            
            <div class="client-nav">
                <ul class="client-filter">
                    <li><a href="#" data-filter="*" class="active">all</a></li>
                    <li><a href="#" data-filter=".case">case studies</a></li>
                    <li><a href="#" data-filter=".integration">integration</a></li>
                    <li><a href="#" data-filter=".financial">financial management</a></li>
                    <li><a href="#" data-filter=".business">business process</a></li>
                </ul>
            </div><!-- / .client-nav -->

            <div class="clearfix"></div>
            <div class="space100"></div>

            <div class="client-subcontainer">
                {{#each model.ourClients as |oClient|}}
                <div class="col-md-3 item-client case clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                    <figure class="alignnone">
                        <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{oClient.Image}}" alt="">
                    </figure>
                </div><!-- / .item-client -->
                {{/each}}
            </div>

        </div><!-- / .clientour-container -->

    </div>
</section>
<!-- SECTION CLIENT-OUR END -->

<!-- SECTION CLIENT-BUSINESS BEGIN -->
<section id="client-business" class="content-section content-clientbusiness nopaddbottom clearfix">

    <div class="bg-white">
        <div class="container">
            
            <div class="row">
                <div class="col-md-6 col-md-offset-3">
                    {{#each model.businessProcessTitle as |processTitle|}}
                    <header class="content-header">
                        <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{processTitle.title}}</h2>
                    </header>
                    <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{processTitle.description}}</p>

                    <div class="clearfix"></div>
                    <div class="space75"></div>

                    <figure class="alignnone">
                        <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{processTitle.Image}}" alt="">
                    </figure>

                    <div class="clearfix"></div>
                    <div class="space100"></div>
                    <div class="space100"></div>
                    <div class="space100"></div>
                    <div class="space100"></div>
                    <div class="space100"></div>
                    {{/each}}
                </div>
            </div><!-- / .row -->

        </div><!-- / .container -->
    </div>

    <div class="content-float bg-gradient-bluewhite">
        <div class="container">

            <div class="row">
                <div class="col-md-10 col-md-offset-1">
                    {{#if model.businessProcess}}
                        {{clients-process bPro=model.businessProcess}}
                    {{/if}}

                    <p class="text-center">
                        <a href="#" class="btn btn-white text-blue">full case</a>
                    </p>

                    <div class="space100"></div>
                    <div class="clearfix"></div>

                    <blockquote class="sciensa-quote">
                        {{#each model.chadRubinson as |people|}}
                        <p class="wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">“{{people.description}}”</p>

                        <figure class="alignnone wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">
                            <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{people.Image}}" alt="">
                        </figure>

                        <h3 class="quote-authors wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms" >{{people.title}}</h3>
                        <span class="quote-titel wow fadeInUp" data-wow-duration="1s" data-wow-delay="300ms">{{people.itemTitle}}</span>
                        {{/each}}
                    </blockquote>

                </div>
            </div>

            <div class="clearfix"></div>
            <div class="space100"></div>
            <div class="space100"></div>

        </div>
    </div><!-- / .content-float -->

</section>
<!-- SECTION CLIENT-BUSINESS END -->

<!-- SECTION INFORMATION BEGIN -->
<section id="information" class="content-section content-information bg-bluedark clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                {{#each model.knowMore as |more|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s"data-wow-delay="200ms" >{{more.title}}</h2>
                </header>

                <div class="clearfix"></div>
                <div class="text-center">
                    <a href="#" class="btn btn-bluelight wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">contact us</a>
                    <div class="space45"></div>
                    <p class="text-uppercase">{{more.phone}}</p>
                </div>
                {{/each}}
            </div>
        </div>

    </div>
</section>
<!-- SECTION INFORMATION END -->
{{app-footer}}