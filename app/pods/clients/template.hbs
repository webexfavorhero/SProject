<!-- SECTION CLIENT-BUSINESS BEGIN -->
<section id="client-business" class="content-section content-clientbusiness nopaddbottom clearfix">

    <div class="bg-white">
        <div class="container">
            
            <div class="row">
                <div class="col-md-6 col-md-offset-3">
                    {{#each model.businessProcessTitle as |processTitle|}}
                    <header class="content-header">
                        <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{processTitle.title}}</h2>
                    </header>
                    <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{processTitle.description}}</p>

                    <div class="clearfix"></div>
                    <div class="space75"></div>

                    <figure class="alignnone">
                        <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{processTitle.Image}}" alt="">
                    </figure>

                    <div class="clearfix"></div>
                    <div class="space100"></div>
                    <div class="space100"></div>
                    <div class="space100"></div>
                    <div class="space100"></div>
                    <div class="space100"></div>
                    {{/each}}
                </div>
            </div><!-- / .row -->

        </div><!-- / .container -->
    </div>

    <div class="content-float bg-gradient-bluewhite">
        <div class="container">

            <div class="row">
                <div class="col-md-10 col-md-offset-1">
                    {{#if model.businessProcess}}
                        {{clients-process bPro=model.businessProcess}}
                    {{/if}}

                    <p class="text-center">
                        <a href="#" class="btn btn-white text-blue">full case</a>
                    </p>

                    <div class="space100"></div>
                    <div class="clearfix"></div>

                    <blockquote class="sciensa-quote">
                        {{#each model.chadRubinson as |people|}}
                        <p class="wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">“{{people.description}}”</p>

                        <figure class="alignnone wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">
                            <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{people.Image}}" alt="">
                        </figure>

                        <h3 class="quote-authors wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms" >{{people.title}}</h3>
                        <span class="quote-titel wow fadeInUp" data-wow-duration="1s" data-wow-delay="300ms">{{people.itemTitle}}</span>
                        {{/each}}
                    </blockquote>

                </div>
            </div>

            <div class="clearfix"></div>
            <div class="space100"></div>
            <div class="space100"></div>

        </div>
    </div><!-- / .content-float -->

</section>
<!-- SECTION CLIENT-BUSINESS END -->

