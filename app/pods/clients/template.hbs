{{app-header-subheader}}
<!-- BANNER HOME BG BEGIN -->
{{#each model.ourClientsBanner as |clientsBanner|}}
<section class="content-section-bg clearfix" style="background-image: url('http://ec2-54-83-80-20.compute-1.amazonaws.com{{clientsBanner.gallery}}');">
    <div class="mask-banner"></div>
    <div class="container">
        <div class="caption-banner" style="max-width: 680px;">
            <h1 class="title-banner wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{clientsBanner.title}}</h1>
            <p class="text-banner wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms" >{{clientsBanner.description}}</p>
        </div><!-- / .caption-banner -->
    </div>

    <a class="link-arrow">
        <i class="fa fa-angle-double-down"></i>
    </a>
</section>
{{/each}}
<!-- BANNER HOME BG END -->

<!-- SUBHEADER BEGIN -->
<div class="outer-subheader">
    <div class="site-subheader bg-blue clearfix">
        <div class="container">
            
            <div class="subheader-navigation">
                <ul>
                    <li><a href="clients#client-introduce">about clients</a></li>
                    <li><a href="clients#client-our">our clients</a></li>
                    <li><a href="clients#client-business">business process</a></li>

                    <!-- <li><a href="clients#">we are sciensa</a></li>
                    <li><a href="clients#">mission</a></li>
                    <li><a href="clients#">culture</a></li>
                    <li><a href="clients#">what we do</a></li>
                    <li><a href="clients#">latested cases</a></li> -->
                </ul>
            </div><!-- / .subheader-navigation -->

            <div class="rwd-internal-header clearfix">
                <span class="btn-showing-internal"><i class="fa fa-bars"></i> pages internal</span>
                
                <div class="clearfix"></div>
                <ul class="rwd-internal-navigation"></ul>
            </div>

            <div class="pull-right">
                <a href="#" class="btn btn-white text-blue">contact us</a>
            </div>

        </div>
    </div>
</div>
<!-- SUBHEADER END -->

<!-- SECTION CLIENT-INTRODUCE BEGIN -->
<section id="client-introduce" class="content-section content-clientintroduce clearfix">
    <div class="container">

        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                {{#each model.aboutClients as |aClients|}}
                <header class="content-header">
                    <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{aClients.gallery}}" alt="" class="icon-content-header wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms" width="186">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{aClients.title}}</h2>
                </header>

                <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{aClients.description}}</p>
                {{/each}}
            </div>
        </div><!-- / .row -->

    </div>
</section>
<!-- SECTION CLIENT-INTRODUCE END -->

