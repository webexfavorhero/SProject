                <div class="text-center">
                    <a href="#" class="btn btn-bluelight wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">contact us</a>
                    <div class="space45"></div>
                    <p class="text-uppercase">{{more.phone}}</p>
                </div>
                {{/each}}
            </div>
        </div>

    </div>
</section>
