<div class="row partners-container">
    <div class="col-md-10 col-md-offset-1">
        
        <div class="partners-carousel owl-carousel">
            {{#each partners as |partner|}}
            <div class="item">
                <blockquote class="sciensa-quote">
                    <p class="wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms" >“{{partner.description}}”</p>

                    <figure class="alignnone wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms" >
                        <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{partner.Image}}" alt="">
                    </figure>

                    <h3 class="quote-authors wow fadeInUp" data-wow-duration="1s" data-wow-delay="300ms">{{partner.title}}</h3>
                    <span class="quote-titel wow fadeInUp" data-wow-duration="1s" data-wow-delay="350ms">{{partner.itemTitle}}</span>
                </blockquote>
            </div>
            {{/each}}
        </div>

    </div>
</div>