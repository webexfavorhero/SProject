<!-- CONTACT-FORM BEGIN -->
<section id="contact" class="content-section content-contactform bg-gradient-pink">
    <div class="container">
        <div class="row contact-container">

            <div class="col-md-10 col-md-offset-1">
                {{#each model.weCan as |cForm|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{cForm.title}}</h2>
                </header>

                <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms" style="max-width: 855px; font-size: 24px;">{{cForm.description}}</p>
                {{/each}}
            </div>

            <div class="clearfix"></div>
            <div class="space100"></div>

            <div class="col-md-10 col-md-offset-1">
                <form class="form-contact">
                    <div class="row row-inner">
                        
                        <div class="col-md-6 clearfix wow fadeInLeft" data-wow-duration="1s" data-wow-delay="250ms">
                            <div class="field-box">
                                <input type="text" placeholder="first name*" required>
                            </div>
                        </div><!-- / .col-md-6 -->

                        <div class="col-md-6 clearfix wow fadeInRight" data-wow-duration="1s" data-wow-delay="250ms">
                            <div class="field-box">
                                <input type="text" placeholder="last name">
                            </div>
                        </div><!-- / .col-md-6 -->

                        <div class="col-md-6 clearfix wow fadeInLeft" data-wow-duration="1s" data-wow-delay="250ms">
                            <div class="field-box">
                                <input type="email" placeholder="email*" required>
                            </div>
                        </div><!-- / .col-md-6 -->

                        <div class="col-md-6 clearfix wow fadeInLeft" data-wow-duration="1s" data-wow-delay="250ms">
                            <div class="field-box">
                                <input type="text" placeholder="phone number" required>
                            </div>
                        </div><!-- / .col-md-6 -->

                        <div class="col-md-6 clearfix wow fadeInRight" data-wow-duration="1s" data-wow-delay="250ms" >
                            <div class="field-box">
                                <input type="text" placeholder="company*" required>
                            </div>
                        </div><!-- / .col-md-6 -->

                        <div class="col-md-6 clearfix wow fadeInLeft" data-wow-duration="1s" data-wow-delay="250ms">
                            <div class="field-box">
                                <input type="text" placeholder="job title" required>
                            </div>
                        </div><!-- / .col-md-6 -->

                        <div class="col-md-12 clearfix wow fadeInUp" data-wow-delay="250ms" data-wow-duration="1s">
                            <div class="field-box">
                                <textarea rows="3" placeholder="message"></textarea>
                            </div>
                        </div><!-- / .col-md-12 -->

                        <div class="col-md-12 clearfix text-center wow fadeInUp " data-wow-duration="1s" data-wow-delay="250ms">
                            <input type="submit" class="btn btn-white text-pink" value="send message">
                        </div><!-- / .col-md-12 -->

                    </div>
                </form><!-- / .form-contact -->
            </div>

        </div><!-- / .contact-container -->
    </div>
</section>
<!-- CONTACT-FORM END -->


