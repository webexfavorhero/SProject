{{#if model.bePart}}
{{app-header}}
<!-- BANNER HOME BG BEGIN -->
<section class="content-section-bg clearfix" style="background-image: url('assets/images/banner-slider/6.jpg');">
    <div class="mask-banner"></div>
    <div class="container">
        <div class="caption-banner" style="max-width: 680px;">
            {{#each model.bePart as |banner|}}
            <h1 class="title-banner wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{banner.title}}</h1>
            <p class="text-banner wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{banner.description}}</p>
            {{/each}}
        </div><!-- / .caption-banner -->
    </div>

    <a class="link-arrow">
        <i class="fa fa-angle-double-down"></i>
    </a>
</section>
<!-- BANNER HOME BG END -->

<!-- HIRE-ENTERPRISE BEGIN -->
<section id="hire-enterprise" class="content-section content-hireenterprise clearfix">
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                {{#each model.buildingDisTitle as |disTitle|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{disTitle.title}}</h2>
                </header>
                {{/each}}
            </div>
        </div><!-- / .row -->

        <div class="row hireenterprise-container">
            {{#each model.buildingDis as |dis|}}
            <div class="col-md-4 item-enterprise clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                <div class="subitem-enterprise">
                    <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{dis.gallery}}" width="130" alt="">
                    <h3><a href="#">{{dis.itemTitle}}</a></h3>
                    <p>{{dis.itemDescription}}</p>
                </div>
            </div><!-- / .item-enterprise -->
            {{/each}}
        </div><!-- / .hireenterprise-container -->

    </div>
</section>
<!-- HIRE ENTERPRISE END -->

