<section id="home-articles" class="content-section content-homearticles clearfix">
  <div class="container">

    <div class="row">
      <div class="col-md-10 col-md-offset-1">
        {{#each model.loremIpsum as |lorem|}}
        <header class="content-header">
          <h2 class="title-content">{{lorem.title}}</h2>
        </header>
        {{/each}}
      </div>
    </div><!-- / .row -->

    <div class="row articles-container">
      {{#each model.blogs as |blog|}}
      <div class="col-md-4 item-articles wow fadeInLeft" data-wow-duration="1s" data-wow-delay="250ms">
        <div class="subitem-articles clearfix">
          <div class="caption-articles">
            <header>
              <p class="meta-extra">{{blog.postDate}}</p>
              <h3>{{blog.postTitle}}</h3>
            </header>
            <a href="blog-post/{{blog.postId}}" class="link">
              <i class="fa fa-mail-forward"></i>
            </a>
            <footer>
              <figure>
                <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{blog.authorImage}}" alt="">
              </figure>
              <p class="meta-authors">By {{blog.author}}</p>
            </footer>
          </div>
          <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{blog.featureImage}}" alt="">
        </div>
      </div><!-- / .item-articles -->
      {{/each}}

    </div><!-- / .articles-container -->

    <div class="clearfix"></div>
    <div class="space75"></div>
    <div class="text-center">
      <a href="#" class="btn btn-line-gray">see our awesome articles</a>
    </div>

  </div>
</section>
{{app-footer}}

{{else}}
    {{partial "partials/lazy-loading"}}
{{/if}}