              <header class="header-lightbox modal-header-lightbox bg-blue">
                  <div class="container">
                      
                      <a href="#" class="logo-abs">
                          <img src="assets/images/logo-white-small.png" alt="">
                      </a>

                      <div class="nav-arrow">
                          <a href="#"><i class="fa fa-angle-left"></i></a>
                          <span class="text">{{case.name}}</span>
                          <a href="#"><i class="fa fa-angle-right"></i></a>
                      </div>

                  </div>
              </header>
              <!-- HEADER-LIGHTBOX END -->

              <!-- BANNER LIGHTBOX BEGIN -->
              <section class="content-banner-lightbox custom-content-banner-lightbox clearfix" style="background-image: url('http://ec2-54-83-80-20.compute-1.amazonaws.com{{case.Image}}')">
                  <div class="mask-banner"></div>
                  <div class="container">
                      <div class="caption-lightbox">
                          <!-- <figure class="integration-logo wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                              <img src="assets/images/google.png">
