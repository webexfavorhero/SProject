{{app-header}}
<!-- BANNER HOME BG BEGIN -->
<section class="content-section-bg clearfix" style="background-image: url('assets/images/banner-slider/8.jpg');">
    <div class="mask-banner"></div>
    <div class="container">
        <div class="caption-banner" style="max-width: 680px;">
            {{#each model.labsBanner as |banner|}}
            <h1 class="title-banner wow fadeInUp" data-wow-duration="1s" ata-wow-delay="200ms" >{{banner.title}}</h1>
            <p class="text-banner wow fadeInUp " data-wow-duration="1s" data-wow-delay="250ms">{{banner.description}}</p>
            {{/each}}
        </div><!-- / .caption-banner -->
    </div>

    <a class="link-arrow">
        <i class="fa fa-angle-double-down"></i>
    </a>
</section>
<!-- BANNER HOME BG END -->

<!-- SUBHEADER BEGIN -->
<div class="outer-subheader">
    <div class="site-subheader bg-yellow clearfix">
        <div class="container">
            
            <div class="subheader-navigation">
                <ul>
                    <li><a href="#">about labs</a></li>
                    <li><a href="#">our companies</a></li>
                    <li><a href="#">last cases</a></li>
                </ul>
            </div><!-- / .subheader-navigation -->

            <div class="rwd-internal-header clearfix">
                <span class="btn-showing-internal"><i class="fa fa-bars"></i> pages internal</span>
                
                <div class="clearfix"></div>
                <ul class="rwd-internal-navigation"></ul>
            </div>

            <div class="pull-right">
                <a href="#" class="btn btn-white text-yellow">see more</a>
            </div>

        </div>
    </div>
</div>
<!-- SUBHEADER END -->

<!-- SECTION LABS-INTRODUCE BEGIN -->
<section id="labs-introduce" class="content-section content-labsintroduce clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                {{#each model.aboutLabs as |aLabs|}}
                <header class="content-header">
                    <img src="assets/images/title-labs.png" alt="" class="icon-content-header wow fadeInUp " data-wow-duration="1s" data-wow-delay="200ms" width="186">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{aLabs.title}}</h2>
                </header>

                <p class="text-content-section wow fadeInUp" data-wow-delay="300ms" data-wow-duration="1s" >{{aLabs.description}}</p>
                {{/each}}
            </div>
        </div><!-- / .row -->

    </div>
</section>
<!-- SECTION LABS-INTRODUCE END -->

<!-- SECTION LABS-COMPANIES BEGIN -->
<section id="labs-companies" class="content-section content-labscompanies bg-gray clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                {{#each model.ourCompanies as |ourCom|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{ourCom.title}}</h2>
                </header>

                <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{ourCom.description}}</p>
                {{/each}}
            </div>
        </div>

        <div class="clearfix"></div>
        <div class="space100"></div>

        <div class="row labscompanies-container">
            
            <div class="labscompanies-nav">
                <ul class="labscompanies-filter">
                    <li><a href="#" data-filter="*" class="active">all</a></li>
                    <li><a href="#" data-filter=".communication">communication</a></li>
                    <li><a href="#" data-filter=".social">social</a></li>
                    <li><a href="#" data-filter=".education">education</a></li>
                    <li><a href="#" data-filter=".ecommerce">ecommerce</a></li>
                </ul>
            </div><!-- / .labscompanies-nav -->

            <div class="space100"></div>

            <div class="labscompanies-subcontainer">
                
                <div class="col-md-3 item-labscompanies communication clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                    <figure class="alignnone">
                        <img src="assets/images/clients/1.png" alt="">
                    </figure>
                </div><!-- / .item-labscompanies -->

                <div class="col-md-3 item-labscompanies communication clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                    <figure class="alignnone">
                        <img src="assets/images/clients/2.png" alt="">
                    </figure>
                </div><!-- / .item-labscompanies -->

                <div class="col-md-3 item-labscompanies communication clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                    <figure class="alignnone">
                        <img src="assets/images/clients/3.png" alt="">
                    </figure>
                </div><!-- / .item-labscompanies -->

                <div class="col-md-3 item-labscompanies communication clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                    <figure class="alignnone">
                        <img src="assets/images/clients/4.png" alt="">
                    </figure>
                </div><!-- / .item-labscompanies -->

                <div class="col-md-3 item-labscompanies communication clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                    <figure class="alignnone">
                        <img src="assets/images/clients/5.png" alt="">
                    </figure>
                </div><!-- / .item-labscompanies -->

                <div class="col-md-3 item-labscompanies communication clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                    <figure class="alignnone">
                        <img src="assets/images/clients/6.png" alt="">
                    </figure>
                </div><!-- / .item-labscompanies -->

                <div class="col-md-3 item-labscompanies communication clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                    <figure class="alignnone">
                        <img src="assets/images/clients/7.png" alt="">
                    </figure>
                </div><!-- / .item-labscompanies -->

                <div class="col-md-3 item-labscompanies communication clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                    <figure class="alignnone">
                        <img src="assets/images/clients/2.png" alt="">
                    </figure>
                </div><!-- / .item-labscompanies -->

            </div>

        </div><!-- / .labscompanies-container -->

    </div>
</section>
<!-- SECTION LABS-COMPANIES END -->

<!-- SECTION INFORMATION BEGIN -->
<section id="information" class="content-section content-information bg-bluedark clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                {{#each model.knowMore as |more|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{more.title}}</h2>
                </header>

                <div class="clearfix"></div>
                <div class="text-center">
                    <a href="#" class="btn btn-yellow wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{more.phone}}</a>
                    <div class="space45"></div>
                    <p class="text-uppercase wow fadeInUp" data-wow-duration="1s" data-wow-delay="300ms">or call +55 (11) 3142-9092</p>
                </div>
                {{/each}}
            </div>
        </div>

    </div>
</section>
<!-- SECTION INFORMATION END -->
{{app-footer}}