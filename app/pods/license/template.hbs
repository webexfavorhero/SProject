<!-- BANNER HOME BG BEGIN -->
{{#each model.licensing as |lic|}}
<section class="content-section-bg clearfix" style="background-image: url('http://ec2-54-83-80-20.compute-1.amazonaws.com{{lic.gallery}}');">
    <div class="mask-banner"></div>
    <div class="container">
        <div class="caption-banner" style="max-width: 680px;">
            <h1 class="title-banner wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{lic.title}}</h1>
            <p class="text-banner wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{lic.description}}</p>
        </div><!-- / .caption-banner -->
    </div>

    <a class="link-arrow">
        <i class="fa fa-angle-double-down"></i>
    </a>
</section>
{{/each}}
<!-- BANNER HOME BG END -->

<!-- SUBHEADER BEGIN -->
<div class="outer-subheader">
    <div class="site-subheader bg-green clearfix">
        <div class="container">
            
            <div class="subheader-navigation">
                <ul>
                    <li><a href="#">about</a></li>
                    <li><a href="#">about license</a></li>
                    <li><a href="#">our partners</a></li>
                    <li><a href="#">we licensing</a></li>
                </ul>
            </div><!-- / .subheader-navigation -->

            <div class="rwd-internal-header clearfix">
                <span class="btn-showing-internal"><i class="fa fa-bars"></i> pages internal</span>
                
                <div class="clearfix"></div>
                <ul class="rwd-internal-navigation"></ul>
            </div>

            <div class="pull-right">
                <a href="#" class="btn btn-white text-green">contact us</a>
            </div>

        </div>
    </div>
</div>
<!-- SUBHEADER END -->

<!-- SECTION LICENSE-INTRODUCE BEGIN -->
<section id="license-introduce" class="content-section content-licenseintroduce clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-10 col-md-offset-1">
                {{#each model.aboutLicese as |aLic|}}
                <header class="content-header">
                    <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{aLic.gallery}}" alt="" class="icon-content-header wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms" width="186">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{aLic.title}}</h2>
                </header>

                <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="300ms">{{aLic.description}}</p>
                {{/each}}
            </div>
        </div><!-- / .row -->

    </div>
</section>
<!-- SECTION LICENSE-INTRODUCE END -->

<!-- SECTION LICENSE-PARTNER BEGIN -->
<section id="partner" class="content-section content-licensepartners bg-green clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                {{#each model.ourPartnersTitle as |partnerTitle|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{partnerTitle.title}}</h2>
                </header>
                {{/each}}
            </div>
        </div><!-- / .row -->
        {{#if model.ourPartners}}
            {{license-partner partners=model.ourPartners}}
        {{/if}}

    </div>
</section>
<!-- SECTION LICENSE-PARTNER END -->

<!-- SECTION LICENSE-PARTNERS BEGIN -->
<section id="license-partner" class="content-section content-licensepartners clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                {{#each model.weLicensingTitle as |wLicTitle|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{wLicTitle.title}}</h2>
                </header>

                <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{wLicTitle.description}}</p>
                {{/each}}
            </div>
        </div>

        <div class="clearfix"></div>
        <div class="space100"></div>

        <div class="row licensepartner-container">
            
            <div class="licensepartner-nav">
                <ul class="licensepartner-filter">
                    <li><a href="#" data-filter="*" class="active">all</a></li>
                    <li><a href="#" data-filter=".business">business process</a></li>
                    <li><a href="#" data-filter=".integration">integration</a></li>
                    <li><a href="#" data-filter=".financial">financial management</a></li>
                </ul>
            </div><!-- / .licensing-nav -->
            
            <div class="clearfix"></div>
            <div class="space100"></div>

            <div class="licensepartner-subcontainer">
                {{#each model.weLicensing as |wLic|}}
                <div class="col-md-3 item-licensing business clearfix">
                    <figure class="alignnone">
                        <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{wLic.Image}}" alt="{{wLic.title}}">
                    </figure>
                </div><!-- / .item-licensing -->
                {{/each}}
            </div>

        </div><!-- / .licensepartner-container -->

    </div>
</section>
<!-- SECTION LICENSE-PARTNERS END -->

<!-- SECTION INFORMATION BEGIN -->
<section id="information" class="content-section content-information bg-bluedark clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                {{#each model.knowMore as |more|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{more.title}}</h2>
                </header>

                <div class="clearfix"></div>
                <div class="text-center">
                    <a href="#" class="btn btn-green wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">contact us</a>
                    <div class="space45"></div>
                    <p class="text-uppercase wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{more.phone}}</p>
                </div>
                {{/each}}
            </div>
        </div>

    </div>
</section>