<!-- BANNER HOME BG BEGIN -->
{{#each model.weWorkTogether as |weWork|}}
<section class="content-section-bg clearfix" style="background-image: url('http://ec2-54-83-80-20.compute-1.amazonaws.com{{weWork.gallery}}');">
    <div class="mask-banner"></div>
    <div class="container">
        <div class="caption-banner">
            <h1 class="title-banner wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{weWork.title}}</h1>

            <a href="#" class="btn btn-pink wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">contact us</a>
        </div><!-- / .caption-banner -->
    </div>
</section>
{{/each}}
<!-- BANNER HOME BG END -->

<!-- SECTION TEAM-PERSON BEGIN -->
<section id="team-person" class="content-section content-teamperson clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                {{#each model.thisIsOurTitle as |thisIs|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{thisIs.title}}</h2>
                </header>

                <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{thisIs.description}}</p>
                {{/each}}
            </div>
        </div>

        <div class="clearfix"></div>
        <div class="space100"></div>

        <div class="row teamperson-container">
            {{#each model.thisIsOur as |thisIsOur|}}
            <div class="col-md-3 item-person clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms" >
                <div class="subitem-person">
                    <figure>
                        <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{thisIsOur.Image}}" alt="">
                    </figure>
                    <h3>{{thisIsOur.title}}</h3>
                    <span class="person-titel">{{thisIsOur.itemTitle}}</span>
                </div>
            </div><!-- / .item-person -->
            {{/each}}
        </div><!-- / .teamperson-container -->

    </div>
</section>
<!-- SECTION TEAM-PERSON END -->

<!-- SECTION INFORMATION BEGIN -->
<section id="information" class="content-section content-information bg-bluedark clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                {{#each model.comeBe as |be|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{be.title}}</h2>
                </header>

                <div class="clearfix"></div>
                <div class="text-center">
                    <a href="#" class="btn btn-pink wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms" >hire me!</a>
                    <div class="space45"></div>
                    <p class="text-uppercase wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{be.phone}}</p>
                </div>
                {{/each}}
            </div>
        </div>

    </div>
</section>
<!-- SECTION INFORMATION END -->