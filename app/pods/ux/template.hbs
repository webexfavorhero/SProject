{{app-header-subheader}}
{{#if model.userExp}}
    {{ux-banner banners=model.userExp}}
{{/if}}

<!-- SUBHEADER BEGIN -->
<div class="outer-subheader">
    <div class="site-subheader bg-green-light clearfix">
        <div class="container">
            
            <div class="subheader-navigation">
                <ul>
                    <li><a href="ux#ux-introduce">about</a></li>
                    <li><a href="ux#ux-process">process</a></li>
                    <li><a href="ux#ux-quote">quote</a></li>
                    <li><a href="ux#ux-whatwedo">what we do</a></li>
                    <li><a href="ux#last-cases">last cases</a></li>

                    <!-- <li><a href="ux#">about</a></li>
                    <li><a href="ux#">process</a></li>
                    <li><a href="ux#">culture</a></li>
                    <li><a href="ux#">ux</a></li>
                    <li><a href="ux#">smart apis & iot</a></li>
                    <li><a href="ux#">business process</a></li>
                    <li><a href="ux#">data science & hpc</a></li>
                    <li><a href="ux#">devops and cloud</a></li> -->
                </ul>
            </div><!-- / .subheader-navigation -->

            <div class="rwd-internal-header clearfix">
                <span class="btn-showing-internal"><i class="fa fa-bars"></i> pages internal</span>
                
                <div class="clearfix"></div>
                <ul class="rwd-internal-navigation"></ul>
            </div>

            <div class="pull-right">
                <a href="#" class="btn btn-white text-green-light">contact us</a>
            </div>

        </div>
    </div>
</div>
<!-- SUBHEADER END -->

<!-- SECTION UX-INTRODUCE BEGIN -->
<section id="ux-introduce" class="content-section content-uxintroduce  clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-10 col-md-offset-1">
                {{#each model.aboutUser as |aUser|}}
                <header class="content-header">
                    <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{aUser.gallery}}" alt="" class="icon-content-header wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms" width="186">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{aUser.title}}</h2>
                </header>

                <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="300ms">{{aUser.description}}</p>
                {{/each}}
            </div>
        </div>

    </div>
</section>
<!-- SECTION UX-INTRODUCE END -->

<!-- SECTION UX-PROCESS BEGIN -->
<section id="ux-process" class="content-section content-uxprocess bg-gradient-green-light clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-10 col-md-offset-1">
                {{#each model.productProTitle as |proTitle|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{proTitle.title}}</h2>
                </header>

                <p class="text-content-section text-italic wow fadeInUp" style="max-width: 855px; font-size: 24px;" data-wow-duration="1s" data-wow-delay="250ms">“{{proTitle.description}} ”</p>
                {{/each}}
            </div>
        </div>

        <div class="clearfix"></div>
        <div class="space100"></div>

        <div class="row uxprocess-container">
            {{#each model.productPro as |pro|}}
            <div class="col-md-3 item-mission clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                <div class="subitem-mission">
                    <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{pro.gallery}}" alt="" width="125">
                    <h3><a href="#">{{pro.itemTitle}}</a></h3>
                </div>                        
            </div><!-- / .item-mission -->
            {{/each}}
        </div><!-- / .uxprocess-container -->

    </div>
</section>
<!-- SECTION PRODUCT-PROCESS END -->

<!-- SECTION UX-QUOTE BEGIN -->
<section id="ux-quote" class="content-section-bg content-uxquote clearfix" style="background-image: url('assets/images/banner-section/2.jpg');">
    <div class="mask-section"></div>
    <div class="container">
        <div class="row">
            {{#each model.loremIps as |lorem|}}
            <div class="col-md-10 col-md-offset-1">
                <div class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{lorem.title}}</h2>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</section>
<!-- SECTION UX-QUOTE END -->



<!-- SECTION UX-WHATWEDO BEGIN -->
<section id="ux-whatwedo" class="content-section content-uxwhatwedo bg-gray clearfix">
    <div class="container">

        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                {{#each model.whatWeTitle as |weTitle|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{weTitle.title}}</h2>
                </header>

                <p class="text-content-section wow fadeInUp" style="max-width: 655px;" data-wow-duration="1s" data-wow-delay="250ms">{{weTitle.description}}</p>
                {{/each}}
            </div>    
        </div>

        <div class="clearfix"></div>
        <div class="space100"></div>
        <div class="row uxwhatwedo-container">
            {{#each model.whatWe as |we|}}
            <div class="col-md-6 item-whatwedo clearfix wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">
                <div class="subitem-whatwedo">
                    <figure>
                        <img src="http://ec2-54-83-80-20.compute-1.amazonaws.com{{we.gallery}}" width="105" alt="">
                    </figure>

                    <div class="desc">
                        <h3><a href="#">{{we.itemTitle}}</a></h3>
                        <p>{{we.itemDescription}}</p>
                    </div><!-- / .desc -->
                </div>
            </div><!-- / .item-whatwedo -->
            {{/each}}
        </div><!-- / .uxwhatwedo-container -->

    </div>
</section>
<!-- SECTION PRODUCT-CULTURE END -->

<!-- SECTION UX-LASTCASES BEGIN -->
<section id="last-cases" class="content-section content-uxlastcases clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                {{#each model.latestCasesTitle as |casesTitle|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{casesTitle.title}}</h2>
                </header>

                <p class="text-content-section wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">{{casesTitle.description}}</p>
                {{/each}}
            </div>
        </div><!-- / .row -->

        <div class="clearfix"></div>
        <div class="space100"></div>

        {{ux-lastcases}}

    </div>
</section>
<!-- SECTION UX-LASTCASES END -->

<!-- SECTION INFORMATION BEGIN -->
<section id="information" class="content-section content-information bg-bluedark clearfix">
    <div class="container">
        
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                {{#each model.knowMore as |more|}}
                <header class="content-header">
                    <h2 class="title-content wow fadeInUp" data-wow-duration="1s" data-wow-delay="200ms">{{more.title}}</h2>
                </header>

                <div class="clearfix"></div>
                <div class="text-center">
                    <a href="#" class="btn btn-green-light wow fadeInUp" data-wow-duration="1s" data-wow-delay="250ms">see more</a>
                    <div class="space45"></div>
                    <p class="text-uppercase wow fadeInUp" data-wow-duration="1s" data-wow-delay="300ms">{{more.phone}}</p>
                </div>
                {{/each}}
            </div>
        </div>

    </div>
</section>
<!-- SECTION INFORMATION END -->
{{app-footer}}